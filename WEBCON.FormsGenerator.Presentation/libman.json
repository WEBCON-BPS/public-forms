{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "bootstrap@5.3.3",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/css/bootstrap-grid.css",
        "dist/css/bootstrap-grid.css.map",
        "dist/css/bootstrap-grid.min.css",
        "dist/css/bootstrap-grid.min.css.map",
        "dist/css/bootstrap-reboot.css",
        "dist/css/bootstrap-reboot.css.map",
        "dist/css/bootstrap-reboot.min.css",
        "dist/css/bootstrap-reboot.min.css.map",
        "dist/css/bootstrap.css",
        "dist/css/bootstrap.css.map",
        "dist/css/bootstrap.min.css",
        "dist/css/bootstrap.min.css.map",
        "dist/js/bootstrap.bundle.js",
        "dist/js/bootstrap.bundle.js.map",
        "dist/js/bootstrap.bundle.min.js",
        "dist/js/bootstrap.bundle.min.js.map",
        "dist/js/bootstrap.js",
        "dist/js/bootstrap.js.map",
        "dist/js/bootstrap.min.js",
        "dist/js/bootstrap.min.js.map"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@fortawesome/fontawesome-free@5.15.2",
      "destination": "wwwroot/lib/fontawesome/",
      "files": [
        "css/all.css",
        "css/all.min.css",
        "css/fontawesome.min.css",
        "css/brands.css",
        "css/brands.min.css",
        "css/solid.min.css",
        "webfonts/fa-solid-900.eot",
        "webfonts/fa-solid-900.ttf",
        "webfonts/fa-solid-900.woff",
        "webfonts/fa-solid-900.woff2",
        "svgs/solid/calendar-alt.svg"
      ]
    },
    {
      "library": "jquery@3.7.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js",
        "jquery.js"
      ]
    },
    {
      "library": "jquery-validate@1.17.0",
      "destination": "wwwroot/lib/jquery-validation/",
      "files": [
        "jquery.validate.min.js",
        "localization/messages_pl.js",
        "localization/messages_pl.min.js",
        "jquery.validate.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    },
    {
      "library": "moment.js@2.29.1",
      "destination": "wwwroot/lib/moment.js/"
    },
    {
      "library": "jquery-datetimepicker@2.5.20",
      "destination": "wwwroot/lib/jquery-datetimepicker/",
      "files": [
        "jquery.datetimepicker.css",
        "jquery.datetimepicker.full.js",
        "jquery.datetimepicker.full.min.js",
        "jquery.datetimepicker.js",
        "jquery.datetimepicker.min.css",
        "jquery.datetimepicker.min.js"
      ]
    }
  ]
}